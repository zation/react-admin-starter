{
  "name": "react-admin-starter",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "build": "npm run public && NODE_ENV=production webpack && node ./html.js",
    "start": "npm run public && cp src/index.html public/ && webpack-dev-server --open",
    "public": "rm -rf public && mkdir public && cp src/favicon.ico public/",
    "mocker": "BABEL_ENV=mocker nodemon --watch ./mocker ./mocker/index.js --exec babel-node"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less,scss,sss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "2.12.2",
    "babel-runtime": "6.25.0",
    "classnames": "2.2.5",
    "es6-promise": "4.1.1",
    "history": "4.6.3",
    "js-cookie": "2.1.4",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "normalizr": "3.2.3",
    "nprogress": "0.2.0",
    "prop-types": "15.5.10",
    "qs": "6.5.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "recharts": "0.22.4",
    "recompose": "0.24.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-form": "7.0.3",
    "universal-router": "^3.2.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-import": "1.2.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-exponentiation-operator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "1.17.2",
    "cors": "2.8.4",
    "css-loader": "0.28.4",
    "editorconfig-tools": "^0.1.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-css-modules": "^2.7.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "4.15.3",
    "express-delay": "0.2.0",
    "faker": "4.1.0",
    "file-loader": "0.11.2",
    "husky": "^0.14.3",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "lint-staged": "^4.0.2",
    "md5": "2.2.1",
    "morgan": "1.8.2",
    "nodemon": "1.11.0",
    "postcss-loader": "2.0.6",
    "react-hot-loader": "3.0.0-beta.7",
    "redux-logger": "3.0.6",
    "style-loader": "0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.6.0",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.6.1",
    "webpack-hot-middleware": "2.18.2"
  }
}
